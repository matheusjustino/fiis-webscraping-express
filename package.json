{
	"name": "fiis-webscraping-express",
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT",
	"scripts": {
		"build": "rm -rf ./dist && babel ./src --extensions \".js,.ts\" --out-dir ./dist --copy-files --no-copy-ignored",
		"start": "node ./dist/main/server.js",
		"start:dev": "ts-node-dev -r tsconfig-paths/register --respawn --ignore-watch node_modules --no-notify src/main/server.ts",
		"lint": "eslint \"**/*.{ts,tsx}\"",
		"lint:fix": "yarn lint --fix"
	},
	"devDependencies": {
		"@babel/cli": "^7.18.10",
		"@babel/core": "^7.18.13",
		"@babel/node": "^7.18.10",
		"@babel/plugin-proposal-class-properties": "^7.18.6",
		"@babel/plugin-proposal-decorators": "^7.18.10",
		"@babel/preset-env": "^7.18.10",
		"@babel/preset-typescript": "^7.18.6",
		"@types/cors": "^2.8.12",
		"@types/express": "^4.17.13",
		"@types/node": "^18.7.14",
		"@types/pino": "^7.0.5",
		"@typescript-eslint/eslint-plugin": "^5.36.1",
		"@typescript-eslint/parser": "^5.36.1",
		"babel-plugin-module-resolver": "^4.1.0",
		"eslint": "^8.23.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-import-resolver-typescript": "^3.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-prettier": "^4.2.1",
		"pino-pretty": "^9.1.0",
		"prettier": "^2.7.1",
		"ts-node-dev": "^2.0.0",
		"tsconfig-paths": "^4.1.0",
		"typescript": "^4.8.2"
	},
	"dependencies": {
		"cors": "^2.8.5",
		"dotenv": "^16.0.2",
		"express": "^4.18.1",
		"pino": "^8.4.2",
		"puppeteer": "^17.1.0",
		"reflect-metadata": "^0.1.13",
		"tsyringe": "^4.7.0"
	}
}
